[{
"id": "hidden",
"class": "toggle",
"style": 
    {
    "position": "fixed", "display": "block",
    "width": "0px", "height": "0px",
    "overflow": "hidden",
    "opacity": "0"
    }
},
{
"id": "x-editor-active",
"class": "editor",
"viewport": "#editor-viewport",
"viewer": "#editor-x-viewer",
"toolbar": "#editor-x-toolbar",
"container": "#editor-container",
"line": "x-line",
"title": "x-title",
"text": ".editorText",
"template":
    {
    "required":
        {
        "type": "tablet",
        "screenWidth": "1280px", "screenHeight": "800px",
        "fullScreen": true
        },
    "is": true,
    "for":
        [{
        "url": "projects/animator_player.html",
        "css": "projects/css/animator_player.css"
        }]
    },
"style":
    {
    "position": "absolute", "display": "block",
    "width": "100%", "height": "100%",
    "hover":
        {
        },
    "container":
        {
        "position": "fixed" "display": "block",
        "width": "80%", "margin": "15%"
        },
    "viewport":
        {
        "position": "absolute" "display": "block",
        "width": "90%", "margin": "5%"
        },
    "viewer":
        {
        "position": "absolute" "display": "block",
        "width": "90%", "margin": "5%"
        },
    "line":
        {
        "position": "relative", "display": "block",
        "height": "16px", "width": "100%", "margin": "0px"
        },
    "text":
        {
        "position": "relative", "display": "block",
        "height": "16px", "width": "100%", 
        "padding": "0px", "margin": "0px",
        "fontFamily": "courier", "fontWeight": "normal",
        "fontSize": "12px",
        "color": "rgb(65,65,65)"
        },
    "title":
        {
        "position": "relative", "display": "block",
        "height": "16px", "width": "100%", 
        "padding": "0px", "margin": "0px",
        "fontFamily": "courier", "fontWeight": "bold",
        "fontSize": "12px",
        "color": "rgb(65,65,65)"
        }
    }
},
{
"id": "x-editor-inactive",
"style":
    {
    "position": "absolute", "display": "block",
    "width": "100%", "height": "100%",
    "container":
        {
        "position": "fixed" "display": "box",
        "width": "80%", "margin": "15%"
        },
    "viewport":
        {
        "position": "absolute" "display": "box",
        "width": "90%", "margin": "5%"
        },
    "line":
        {
        "position": "relative", "display": "block",
        "height": "16px", "width": "100%", "margin": "0px",
        "hover":
            {
            
            }
        }
    }
}]